@model InternVM

<div style="width:100%">

    @using (Html.BeginForm("ContactForm", "Scheduling", null, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
        @Html.ValidationSummary(true, "")


        <div class="form-group">
            @Html.LabelFor(s => s.UserName)

            <div>
                @Html.EditorFor(s => s.UserName)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(s => s.LastName)

            <div>
                @Html.EditorFor(s => s.LastName)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(s => s.FirstName)

            <div>
                @Html.EditorFor(s => s.FirstName)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(s => s.Attorneys)

            <div>
                @Html.EditorFor(s => s.Attorneys)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(s => s.CertifiedDate)

            <div>
                @Html.EditorFor(s => s.CertifiedDate)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(s => s.Status)

            <div>
                @Html.EditorFor(s => s.Status)
            </div>
        </div>

    }
</div>
